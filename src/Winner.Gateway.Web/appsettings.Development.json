{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ReverseProxy": {
    "timeout": 10,
    "excludePages": [ "/api/values" ],
    "mapping": [
      {
        "upstream": "localhost:44330",
        "downstream": [
          {
            "host": "https://localhost:44308",
            "weight": 1
          }
        ]
      },
      {
        "upstream": "sms.send",
        "downstream": [
          {
            "host": "https://localhost:44308/api/values/post",
            "weight": 1
          }
        ]
      }
    ]
  }
}
