{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ReverseProxy": {
    "timeout": 10,
    "excludePages": [ "/api/values" ],
    "mapping": [
      {
        "upstream": "localhost:44368",
        "downstream": [
          {
            "host": "localhost:44308",
            "weight": 1,
            "schema": "https"
          }
        ]
      }
    ]
  }
}
